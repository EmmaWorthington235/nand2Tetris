CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;
   
    PARTS:
    Not(in=sel[2], out=notSel);
    And(a=in, b=notSel, out=inNot2);
    And(a=in, b=sel[2], out=in2);
    DMux4Way(in=inNot2, sel=sel[0..1], a=a, b=b, c=c, d=d);
    DMux4Way(in=in2, sel=sel[0..1], a=e, b=f, c=g, d=h);
}